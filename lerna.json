{
  "version": "0.1.3",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "ignoreChanges": ["ignored-file", "*.md"],
      "graphType": "all",
      "message": "chore(release): publish %s",
      "allowBranch": "master",
      "conventionalCommits": true,
      "exact": true,
      "registry":"https://registry.npmjs.org/"
    }
  }
}
